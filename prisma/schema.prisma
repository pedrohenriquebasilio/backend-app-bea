
generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Charging {
  id            Int       @id @default(autoincrement())
  type          String
  liters        Float
  pricePerLiter Float
  total         Float
  date          DateTime
  createdAt     DateTime  @default(now())
}

